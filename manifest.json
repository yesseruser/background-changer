{
    "manifest_version": 3,
    "name": "Background Changer",
    "description": "Changes background of google.com",
    "version": "1.0.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["/main.css"]
    }]
}